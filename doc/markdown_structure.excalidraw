      "text": "# How to exclude folders from a search with find? #command",
      "fontSize": 28,
      "fontFamily": 8,
      "textAlign": "left",
      "verticalAlign": "top",
      "containerId": null,
      "originalText": "# How to exclude folders from a search with find? #command",
      "autoResize": false,
